{
  "name": "atlantis",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "npm-run-all clean:ms-auth clean:ms-settings clean:ms-catalog clean:web",
    "clean:ms-auth": "rimraf ms-auth/dist",
    "clean:ms-settings": "rimraf ms-settings/dist",
    "clean:ms-catalog": "rimraf ms-catalog/dist",
    "clean:web": "rimraf web/dist",
    "build": "npm-run-all build:ms-auth build:ms-settings build:ms-catalog build:web",
    "build:ms-auth": "cd ms-auth/src && tsc",
    "build:ms-settings": "cd ms-settings/src && tsc",
    "build:ms-catalog": "cd ms-catalog/src && tsc && recursive-copy --overwrite resources ../dist/ms-catalog/src/resources/",
    "build:web": "cd web/ && ng build --configuration production",
    "test": "npm-run-all test:ms-auth test:ms-settings test:ms-catalog test:web",
    "test:ms-auth": "mocha -r ts-node/register ms-auth/test/*.ts --exit",
    "test:ms-settings": "mocha -r ts-node/register ms-settings/test/*.ts --exit",
    "test:ms-catalog": "mocha -r ts-node/register ms-catalog/test/*.ts --exit",
    "test:web": "echo 'no tests yet'",
    "lint": "eslint . --ext .ts,.d.ts",
    "lint:fix": "npm run lint -- --fix",
    "dev:start-ms-auth": "cd ms-auth/src && cross-env DOTENV_CONFIG_PATH=../../.env-dev-db.env ts-node -r dotenv/config index.ts",
    "dev:start-ms-settings": "cd ms-settings/src && cross-env DOTENV_CONFIG_PATH=../../.env-dev-db.env ts-node -r dotenv/config index.ts",
    "dev:start-ms-catalog": "cd ms-catalog/src && cross-env DOTENV_CONFIG_PATH=../../.env-dev-db.env ts-node -r dotenv/config index.ts",
    "dev:start-web": "cd web/ && ng serve --host 0.0.0.0 --open",
    "dev:bot-bridge:start-server": "node bot-bridge/dev-server.js 5656",
    "dev:bot-bridge:start-ngrok": "ngrok http 5656",
    "dev:before-push": "npm run test-and-build && echo Well done - code is ready to be pushed! && echo Good Luck!",
    "test-and-build": "npm-run-all lint test build"
  },
  "author": "",
  "license": "ISC",
  "private": true,
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "dependencies": {
    "adaptive-expressions": "~4.14.1",
    "adaptivecards": "~2.9.0",
